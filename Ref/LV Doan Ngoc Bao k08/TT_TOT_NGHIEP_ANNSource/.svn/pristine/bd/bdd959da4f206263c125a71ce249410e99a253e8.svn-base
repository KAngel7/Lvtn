<Window x:Class="NeuronNetwork.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Neuron Network" Height="384" Width="942">
    <Grid>
        <TabControl Height="353" HorizontalAlignment="Left" Name="tabControl1" VerticalAlignment="Top" Width="920">
            <TabItem Header="Configure" Name="tabConfigure">
                <Grid Height="340">
                    <Label Content="Number of Input Nodes" Height="28" HorizontalAlignment="Left" Margin="316,33,0,0" Name="label1" VerticalAlignment="Top" />
                    <TextBox Height="28" HorizontalAlignment="Left" Margin="528,33,0,0" Name="textBox1" VerticalAlignment="Top" Width="258" />
                    <Label Content="Number of Hidden Nodes" Height="28" HorizontalAlignment="Left" Margin="316,80,0,0" Name="label2" VerticalAlignment="Top" />
                    <TextBox Height="28" HorizontalAlignment="Left" Margin="528,78,0,0" Name="textBox2" VerticalAlignment="Top" Width="258" />
                    <Button Content="New Networks" Height="23" HorizontalAlignment="Left" Margin="62,70,0,0" Name="button1" VerticalAlignment="Top" Width="98" />
                    <Button Content="Load Networks" Height="23" HorizontalAlignment="Left" Margin="62,216,0,0" Name="button2" VerticalAlignment="Top" Width="98" />
                    <Button Content="Edit Networks" Height="23" HorizontalAlignment="Left" Margin="62,140,0,0" Name="button3" VerticalAlignment="Top" Width="98" />
                    <Label Content="Number of Output Nodes" Height="28" HorizontalAlignment="Left" Margin="316,126,0,0" Name="label3" VerticalAlignment="Top" />
                    <Label Content="Weight" Height="28" HorizontalAlignment="Left" Margin="317,176,0,0" Name="label4" VerticalAlignment="Top" />
                    <TextBox Height="28" HorizontalAlignment="Left" Margin="528,126,0,0" Name="textBox3" VerticalAlignment="Top" Width="258" />
                    <TextBox Height="28" HorizontalAlignment="Left" Margin="528,176,0,0" Name="textBox4" VerticalAlignment="Top" Width="258" />
                    <Label Content="Bias" Height="28" HorizontalAlignment="Left" Margin="316,225,0,0" Name="label5" VerticalAlignment="Top" />
                    <TextBox Height="28" HorizontalAlignment="Left" Margin="528,225,0,0" Name="textBox5" VerticalAlignment="Top" Width="258" />
                </Grid>
            </TabItem>
            <TabItem Header="Training" Name="tabTraining">
                <Grid />
            </TabItem>
            <TabItem Header="Testing" Name="tabTesting">
                <Grid />
            </TabItem>
            <TabItem Header="Forecasting" Name="tabForecasting">
                <Grid />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
